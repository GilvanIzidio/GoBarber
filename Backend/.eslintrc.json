{
	"env": {
			"es2021": true,
			"node": true
	},
	"extends": [
			"airbnb-base",
	// "prettier/@typescript-eslint",
	"plugin:@typescript-eslint/recommended",
	"plugin:prettier/recommended"

	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
			"ecmaVersion": 12,
			"sourceType": "module"
	},
	"plugins": [
			"@typescript-eslint",
	"prettier"
	],
	"rules": {
	"prettier/prettier": "warn",
	"class-methods-use-this":"off",
	"no-console":"off",
	"camelcase":"off",
	"indent": [2, "tab"],
	"no-tabs": 0,
	"@typescript-eslint/no-unused-vars":["error",{
		"argsIgnorePattern":"_"
	}],
	"import/no-extraneous-dependencies": [
		"error",
		{
			"devDependencies": true
		}
	],
	"import/extensions": [
		"error",
		"ignorePackages",
		{
			"ts": "never"
		}
	]
	},
"settings": {
	"import/resolver": {
		"typescript": {

		}
	}
}
}
